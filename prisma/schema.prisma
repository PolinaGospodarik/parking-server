generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int    @id @default(autoincrement())
  fullName    String
  phoneNumber String @unique
  password    String
  role        Role   @default(USER)
}

enum Role {
  USER
  ADMIN
  GUARD
}
